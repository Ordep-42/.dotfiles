[alias]
    last = log -1 HEAD
    part = add --patch
	main = checkout main
	stts = status

[init]
	defaultBranch = main

[color]
	ui = auto

[color "status"]
	added = green
	changed = yellow
	untracked = cyan

[color "branch"]
	local = green
	current = green reverse
	remote = yellow

[color "diff"]
	meta = yellow
	frag = magenta
	old = red bold
	new = green bold
	commit = yellow bold

[push]
	autoSetupRemote = true

[pull]
	rebase = false

[filter "lfs"]
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
